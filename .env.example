
# Example connection strings:
# MONGODB_URL://127.0.0.1:27017/devdb
# MONGODB_URL=mongodb://host.docker.internal:27017/devdb
# MONGODB_URL=mongodb://dev:zd12Asd1@127.0.0.1:27017/devdb?authSource=admin
# mongodb://[MongodbHost]:[PORT]/[DatabaseName]
MONGODB_URL=<connection_string>

# Example Secret:-  3okZMmTNr0oxdd1G31wH
# Can be generated from https://randomkeygen.com/
JWT_SECRET=<secret>

# You can place duration available here: https://github.com/auth0/node-jsonwebtoken#usage
# Search for "expiresIn" option on above link.
JWT_TIMEOUT_DURATION="2 hours"


# Admin account email and password
# this account will be generated when you call /auth/init for the first time
# only admin user can register new users
ADMIN_EMAIL=<something@mail.com>
ADMIN_PWD=<pwd>

# SMTP Configuration
EMAIL_SMTP_HOST=smtp.gmail.com
EMAIL_SMTP_PORT=587
EMAIL_SMTP_SECURE=true
EMAIL_SMTP_USERNAME=<something@mail.com>
EMAIL_SMTP_PASSWORD=<pwd>

CLIENT_EMAIL_VERIFY_URL=<url>
